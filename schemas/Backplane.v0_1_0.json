{
    "$schema": "http://redfish.dmtf.org/schemas/v1/redfish-schema.v1_2_0.json",
    "title": "#Backplane.v0_1_0.Backplane",
    "$ref": "#/definitions/Backplane",
    "definitions": {
        "Backplane": {
            "type": "object",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "additionalProperties": false,
            "properties": {
                "@odata.context": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/context"
                },
                "@odata.id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/id"
                },
                "@odata.type": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/type"
                },
                "Oem": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                    "description": "This is the manufacturer/provider specific extension moniker used to divide the Oem object into sections.",
                    "longDescription": "The value of this string shall be of the format for the reserved word *Oem*."
                },
                "Id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Id",
                    "readonly": true
                },
                "Description": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Description"},
                        {"type": "null"}
                    ],
                    "readonly": true
                },
                "Name": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name",
                    "readonly": true
                },
                "Status": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Status"},
                        {"type": "null"}
                    ]
                },
                "Links": {
                    "type": "object",
                    "patternProperties": {
                        "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                            "type": [
                                "array",
                                "boolean",
                                "number",
                                "null",
                                "object",
                                "string"
                            ],
                            "description": "This property shall specify a valid odata or Redfish property."
                        }
                    },
                    "additionalProperties": false,
                    "properties": {
                        "Oem": {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                            "description": "Oem extension object.",
                            "longDescription": "This object represents the Oem property.  All values for resources described by this schema shall comply to the requirements as described in the Redfish specification."
                        },
                        "ComputerSystems@odata.count": {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                        },
                        "ComputerSystems@odata.navigationLink": {
                            "type": "string",
                            "format": "uri"
                        },
                        "ComputerSystems": {
                            "type": "array",
                            "items": {
                                "$ref": "http://redfish.dmtf.org/schemas/v1/ComputerSystem.json#/definitions/ComputerSystem"
                            },
                            "readonly": true,
                            "description": "An array of references to the ComputerSystems hosted on the Baseboards connected to this Backplane.",
                            "longDescription": "This object shall only contain an array of references that comply to the ComputerSystem schema."
                        },
                        "PCIeDevices@odata.count": {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                        },
                        "PCIeDevices@odata.navigationLink": {
                            "type": "string",
                            "format": "uri"
                        },
                        "PCIeDevices": {
                            "type": "array",
                            "items": {
                                "$ref": "http://redfish.dmtf.org/schemas/v1/PCIeDevice.json#/definitions/PCIeDevice"
                            },
                            "readonly": true,
                            "description": "An array of references to the PCIeDevices hosted on the Baseboards connected to this Backplane.",
                            "longDescription": "This object shall only contain an array of references that comply to the PCIeDevice schema."
                        }
                    },
                    "readonly": true,
                    "description": "The links object contains the links to other resources that are related to this resource."
                },
                "BackplaneType": {
                    "anyOf": [
                        {"$ref": "#/definitions/BackplaneType"},
                        {"type": "null"}
                    ],
                    "readonly": true,
                    "description": "The type of the Baseboard."
                },
                "Baseboards@odata.count": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                },
                "Baseboards@odata.navigationLink": {
                    "type": "string",
                    "format": "uri"
                },
                "Baseboards": {
                    "type": "array",
                    "items": {
                        "$ref": "http://redfish.dmtf.org/schemas/v1/Baseboard.json#/definitions/Baseboard"
                    },
                    "readonly": true,
                    "description": "An array of references to the Baseboards this Backplane is connected to.",
                    "longDescription": "This object shall only contain an array of references that comply to the Baseboard schema."
                },
                "PCIeSwitches@odata.count": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                },
                "PCIeSwitches@odata.navigationLink": {
                    "type": "string",
                    "format": "uri"
                },
                "PCIeSwitches": {
                    "type": "array",
                    "items": {
                        "$ref": "http://redfish.dmtf.org/schemas/v1/PCIeSwitch.json#/definitions/PCIeSwitch"
                    },
                    "readonly": true,
                    "description": "An array of references to the PCIeSwitches hosted on this Backplane.",
                    "longDescription": "This object shall only contain an array of references that comply to the PCIeSwitch schema."
                },
                "Chassis": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Chassis.json#/definitions/Chassis",
                    "readonly": true,
                    "description": "The Chassis hosting this Backplane",
                    "longDescription": "This object shall only contain a reference that complies to the Chassis schema."
                }
            },
            "required": [
                "Id",
                "Name"
            ],
            "description": "This is the schema definition for the Backplane resource.",
            "longDescription": "This is the schema definition for the Backplane resource. It represents the properties of a Backplane."
        },
        "BackplaneType": {
            "type": "string",
            "enum": [
                "Ethernet",
                "PCIe"
            ],
            "enumDescriptions": {
                "Ethernet": "This Backplane provides the infrastructure for ethernet communication.",
                "PCIe": "This Backplane provides the infrastructure for PCIe communication."
            }
        }
    },
