{
    "$schema": "http://redfish.dmtf.org/schemas/v1/redfish-schema.v1_2_0.json",
    "title": "#PCIeSwitch.v0_9_0.PCIeSwitch",
    "$ref": "#/definitions/PCIeSwitch",
    "definitions": {
        "Reset": {
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "title": {
                    "type": "string",
                    "description": "Friendly action name"
                },
                "target": {
                    "type": "string",
                    "format": "uri",
                    "description": "Link to invoke action"
                }
            },
            "description": "This action is used to reset the system.",
            "longDescription": "This action shall perform a reset of the PCIeSwitch. For systems which implement APCI Power Button functionality, the PushPowerButton value shall perform or emulate an ACPI Power Button push. The ForceOff value shall remove power from the system or perform an ACPI Power Button Override (commonly known as a 4-second hold of the Power Button). The ForceRestart value shall perform a ForceOff action followed by a On action."
        },
        "IndicatorLED": {
            "type": "string",
            "enum": [
                "Lit",
                "Blinking",
                "Off"
            ],
            "enumDescriptions": {
                "Lit": "The Indicator LED is lit.",
                "Blinking": "The Indicator LED is blinking.",
                "Off": "The Indicator LED is off."
            }
        },
        "PCIeSwitch": {
            "type": "object",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "additionalProperties": false,
            "properties": {
                "@odata.context": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/context"
                },
                "@odata.id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/id"
                },
                "@odata.type": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/type"
                },
                "Oem": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                    "description": "This is the manufacturer/provider specific extension moniker used to divide the Oem object into sections.",
                    "longDescription": "The value of this string shall be of the format for the reserved word *Oem*."
                },
                "Id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Id",
                    "readonly": true
                },
                "Description": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Description"},
                        {"type": "null"}
                    ],
                    "readonly": true
                },
                "Name": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name",
                    "readonly": true
                },
                "Links": {
                    "type": "object",
                    "patternProperties": {
                        "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                            "type": [
                                "array",
                                "boolean",
                                "number",
                                "null",
                                "object",
                                "string"
                            ],
                            "description": "This property shall specify a valid odata or Redfish property."
                        }
                    },
                    "additionalProperties": false,
                    "properties": {
                        "Oem": {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                            "description": "Oem extension object.",
                            "longDescription": "This object represents the Oem property.  All values for resources described by this schema shall comply to the requirements as described in the Redfish specification."
                        },
                        "Chassis": {
                            "anyOf": [
                                {"$ref": "http://redfish.dmtf.org/schemas/v1/ChassisCollection.json#/definitions/ChassisCollection"},
                                {"type": "null"}
                            ],
                            "readonly": true,
                            "description": "This is a link to a collection of chassis in which this switch is contained",
                            "longDescription": "This object shall only contain a reference to a collection of resources that comply to the Chassis schema."
                        },
                        "ManagedBy": {
                            "anyOf": [
                                {"$ref": "http://redfish.dmtf.org/schemas/v1/ManagerCollection.json#/definitions/ManagerCollection"},
                                {"type": "null"}
                            ],
                            "readonly": true,
                            "description": "This is a link to a collection of Managers responsible for this switch.",
                            "longDescription": "This object shall only contain a reference to a collection of resources that comply to the Manager schema."
                        },
                        "PoweredBy": {
                            "anyOf": [
                                {"$ref": "http://redfish.dmtf.org/schemas/v1/Resource.v1_0_0.json#/definitions/ResourceCollection"},
                                {"type": "null"}
                            ],
                            "readonly": true,
                            "description": "An array of ID[s] of resources that power this switch.",
                            "longDescription": "This object shall only contain a reference to a collection of resources that comply to the Resource schema."
                        },
                        "CooledBy": {
                            "anyOf": [
                                {"$ref": "http://redfish.dmtf.org/schemas/v1/Resource.v1_0_0.json#/definitions/ResourceCollection"},
                                {"type": "null"}
                            ],
                            "readonly": true,
                            "description": "An array of ID[s] of resources that cool this switch.",
                            "longDescription": "This object shall only contain a reference to a collection of resources that comply to the Resource schema."
                        }
                    },
                    "readonly": true,
                    "description": "Contains links to other resources that are related to this resource."
                },
                "MaxZones": {
                    "type": [
                        "number",
                        "null"
                    ],
                    "minimum": 0,
                    "readonly": false,
                    "description": "Maximum number of MR zones supported by the switch",
                    "longDescription": ""
                },
                "Manufacturer": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "readonly": true,
                    "description": "The manufacturer or OEM of this Switch.",
                    "longDescription": "The value of this property shall contain a value that represents the manufacturer of the Switch."
                },
                "Model": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "readonly": true,
                    "description": "The model number for this Switch",
                    "longDescription": ""
                },
                "SKU": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "readonly": true,
                    "description": "The manufacturer SKU for this Switch",
                    "longDescription": "The value of this property shall contain the Stock Keeping Unit (SKU) for the Switch."
                },
                "SerialNumber": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "readonly": true,
                    "description": "The serial number for this switch",
                    "longDescription": "The value of this property shall contain the serial number for the switch."
                },
                "PartNumber": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "readonly": true,
                    "description": "The part number for this switch",
                    "longDescription": "The value of this property shall contain the part number for the Switch as defined by the manufacturer."
                },
                "IndicatorLED": {
                    "anyOf": [
                        {"$ref": "#/definitions/IndicatorLED"},
                        {"type": "null"}
                    ],
                    "readonly": false,
                    "description": "The state of the indicator LED, used to identify the Switch",
                    "longDescription": "The value of this property shall contain the indicator light state for the indicator light associated with this Switch."
                },
                "PowerState": {
                    "anyOf": [
                        {"$ref": "#/definitions/PowerState"},
                        {"type": "null"}
                    ],
                    "readonly": true,
                    "description": "This is the current power state of the Switch",
                    "longDescription": "The value of this property shall contain the power state of the switch."
                },
                "Actions": {
                    "type": "object",
                    "patternProperties": {
                        "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                            "type": [
                                "array",
                                "boolean",
                                "number",
                                "null",
                                "object",
                                "string"
                            ],
                            "description": "This property shall specify a valid odata or Redfish property."
                        }
                    },
                    "additionalProperties": false,
                    "properties": {
                        "Oem": {
                            "type": [
                                "object",
                                "null"
                            ],
                            "patternProperties": {
                                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                                    "type": [
                                        "array",
                                        "boolean",
                                        "number",
                                        "null",
                                        "object",
                                        "string"
                                    ],
                                    "description": "This property shall specify a valid odata or Redfish property."
                                }
                            },
                            "additionalProperties": true,
                            "properties": {}
                        },
                        "#PCIeSwitch.Reset": {
                            "$ref": "#/definitions/Reset"
                        }
                    },
                    "readonly": true,
                    "description": "The Actions object contains the available custom actions on this resource."
                },
                "Status": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Status"},
                        {"type": "null"}
                    ]
                },
                "Zones": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/PCIeZoneCollection.json#/definitions/PCIeZoneCollection"},
                        {"type": "null"}
                    ],
                    "readonly": true,
                    "description": "A reference to the collection of Zones associated with this Switch",
                    "longDescription": ""
                },
                "PCIePorts": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/PCIePortCollection.json#/definitions/PCIePortCollection"},
                        {"type": "null"}
                    ],
                    "readonly": true,
                    "description": "A reference to the collection of PCIe Ports associated with this Switch",
                    "longDescription": ""
                },
                "AttachedDevices": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/PCIeDeviceCollection.json#/definitions/PCIeDeviceCollection"},
                        {"type": "null"}
                    ],
                    "readonly": true,
                    "description": "This is a link to a collection of PCIeDevices attached to this switch.",
                    "longDescription": "This object shall only contain a reference to a collection of resources that comply to the PCIeDevice schema."
                },
                "LogServices": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/LogServiceCollection.json#/definitions/LogServiceCollection"},
                        {"type": "null"}
                    ],
                    "readonly": true,
                    "description": "A reference to the collection of Log Services associated with this system",
                    "longDescription": "The value of this property shall be a link to a collection of type LogServiceCollection."
                }
            },
            "required": [
                "Id",
                "Name"
            ],
            "description": "This schema defines a PCIeSwitch and its respective properties.",
            "longDescription": ""
        },
        "PowerState": {
            "type": "string",
            "enum": [
                "On",
                "Off",
                "PoweringOn",
                "PoweringOff"
            ],
            "enumDescriptions": {
                "On": "The system is powered on.",
                "Off": "The system is powered off, although some components may continue to have AUX power such as management controller.",
                "PoweringOn": "A temporary state between Off and On. This temporary state can be very short.",
                "PoweringOff": "A temporary state between On and Off. The power off action can take time while the OS is in the shutdown process."
            }
        }
    },
    "copyright": "Copyright 2014-2017 Distributed Management Task Force, Inc. (DMTF). For the full DMTF copyright policy, see http://www.dmtf.org/about/policies/copyright"
}
