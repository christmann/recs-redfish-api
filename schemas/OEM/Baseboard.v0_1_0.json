{
    "$schema": "http://redfish.dmtf.org/schemas/v1/redfish-schema.v1_2_0.json",
    "title": "#Baseboard.v0_1_0.Baseboard",
    "$ref": "#/definitions/Baseboard",
    "definitions": {
        "Baseboard": {
            "type": "object",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "additionalProperties": false,
            "properties": {
                "@odata.context": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/context"
                },
                "@odata.id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/id"
                },
                "@odata.type": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/type"
                },
                "Oem": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                    "description": "This is the manufacturer/provider specific extension moniker used to divide the Oem object into sections.",
                    "longDescription": "The value of this string shall be of the format for the reserved word *Oem*."
                },
                "Id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Id",
                    "readonly": true
                },
                "Description": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Description"},
                        {"type": "null"}
                    ],
                    "readonly": true
                },
                "Name": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name",
                    "readonly": true
                },
                "Status": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Status"},
                        {"type": "null"}
                    ]
                },
                "Links": {
                    "type": "object",
                    "patternProperties": {
                        "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                            "type": [
                                "array",
                                "boolean",
                                "number",
                                "null",
                                "object",
                                "string"
                            ],
                            "description": "This property shall specify a valid odata or Redfish property."
                        }
                    },
                    "additionalProperties": false,
                    "properties": {
                        "Oem": {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                            "description": "Oem extension object.",
                            "longDescription": "This object represents the Oem property.  All values for resources described by this schema shall comply to the requirements as described in the Redfish specification."
                        },
                        "Chassis": {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Chassis.json#/definitions/Chassis",
                            "readonly": true,
                            "description": "The Chassis hosting this Baseboard",
                            "longDescription": "This object shall only contain a reference that complies to the Chassis schema."
                        }
                    },
                    "readonly": true,
                    "description": "The links object contains the links to other resources that are related to this resource."
                },
                "BaseboardType": {
                    "anyOf": [
                        {"$ref": "#/definitions/BaseboardType"},
                        {"type": "null"}
                    ],
                    "readonly": true,
                    "description": "The type of the Baseboard."
                },
                "ComputerSystems@odata.count": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                },
                "ComputerSystems@odata.navigationLink": {
                    "type": "string",
                    "format": "uri"
                },
                "ComputerSystems": {
                    "type": "array",
                    "items": {
                        "$ref": "http://redfish.dmtf.org/schemas/v1/ComputerSystem.json#/definitions/ComputerSystem"
                    },
                    "readonly": true,
                    "description": "An array of references to the ComputerSystems hosted on this Baseboard.",
                    "longDescription": "This object shall only contain an array of references that comply to the ComputerSystem schema."
                },
                "PCIeDevices@odata.count": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                },
                "PCIeDevices@odata.navigationLink": {
                    "type": "string",
                    "format": "uri"
                },
                "PCIeDevices": {
                    "type": "array",
                    "items": {
                        "$ref": "http://redfish.dmtf.org/schemas/v1/PCIeDevice.json#/definitions/PCIeDevice"
                    },
                    "readonly": true,
                    "description": "An array of references to the PCIeDevices hosted on this Baseboard.",
                    "longDescription": "This object shall only contain an array of references that comply to the PCIeDevice schema."
                },
                "EthernetSwitches@odata.count": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                },
                "EthernetSwitches@odata.navigationLink": {
                    "type": "string",
                    "format": "uri"
                },
                "EthernetSwitches": {
                    "type": "array",
                    "items": {
                        "$ref": "http://redfish.dmtf.org/schemas/v1/EthernetSwitch.json#/definitions/EthernetSwitch"
                    },
                    "readonly": true,
                    "description": "An array of references to the EthernetSwitches hosted on this Baseboard.",
                    "longDescription": "This object shall only contain an array of references that comply to the EthernetSwitch schema."
                },
                "PCIeSwitches@odata.count": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                },
                "PCIeSwitches@odata.navigationLink": {
                    "type": "string",
                    "format": "uri"
                },
                "PCIeSwitches": {
                    "type": "array",
                    "items": {
                        "$ref": "http://redfish.dmtf.org/schemas/v1/PCIeSwitch.json#/definitions/PCIeSwitch"
                    },
                    "readonly": true,
                    "description": "An array of references to the PCIeSwitches hosted on this Baseboard.",
                    "longDescription": "This object shall only contain an array of references that comply to the PCIeSwitch schema."
                },
                "Backplanes@odata.count": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                },
                "Backplanes@odata.navigationLink": {
                    "type": "string",
                    "format": "uri"
                },
                "Backplanes": {
                    "type": "array",
                    "items": {
                        "$ref": "http://redfish.dmtf.org/schemas/v1/Backplane.json#/definitions/Backplane"
                    },
                    "readonly": true,
                    "description": "An array of references to the Backplanes this Baseboard is connected to.",
                    "longDescription": "This object shall only contain an array of references that comply to the Backplane schema."
                }
            },
            "required": [
                "Id",
                "Name"
            ],
            "description": "This is the schema definition for the Baseboard resource.",
            "longDescription": "This is the schema definition for the Baseboard resource. It represents the properties of a Baseboard."
        },
        "BaseboardType": {
            "type": "string",
            "enum": [
                "COMExpress",
                "Apalis",
                "Jetson"
            ],
            "enumDescriptions": {
                "COMExpress": "This Baseboard can host COM Express nodes.",
                "Apalis": "This Baseboard can host Apalis nodes.",
                "Jetson": "This Baseboard can host Jetson nodes."
            }
        }
    },
    "copyright": "Copyright 2014-2017 Distributed Management Task Force, Inc. (DMTF). For the full DMTF copyright policy, see http://www.dmtf.org/about/policies/copyright"
}
