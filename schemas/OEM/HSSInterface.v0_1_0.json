{
    "$schema": "http://redfish.dmtf.org/schemas/v1/redfish-schema.v1_2_0.json",
    "title": "#HSSInterface.v0_1_0.HSSInterface",
    "$ref": "#/definitions/HSSInterface",
    "definitions": {
        "HSSInterface": {
            "type": "object",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "additionalProperties": false,
            "properties": {
                "@odata.context": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/context"
                },
                "@odata.id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/id"
                },
                "@odata.type": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/type"
                },
                "Oem": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                    "description": "This is the manufacturer/provider specific extension moniker used to divide the Oem object into sections.",
                    "longDescription": "The value of this string shall be of the format for the reserved word *Oem*."
                },
                "Id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Id",
                    "readonly": true
                },
                "Description": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Description"},
                        {"type": "null"}
                    ],
                    "readonly": true
                },
                "Name": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name",
                    "readonly": true
                },
                "Status": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Status"},
                        {"type": "null"}
                    ]
                },
                "Links": {
                    "type": "object",
                    "patternProperties": {
                        "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                            "type": [
                                "array",
                                "boolean",
                                "number",
                                "null",
                                "object",
                                "string"
                            ],
                            "description": "This property shall specify a valid odata or Redfish property."
                        }
                    },
                    "additionalProperties": false,
                    "properties": {
                        "Oem": {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                            "description": "Oem extension object.",
                            "longDescription": "This object represents the Oem property.  All values for resources described by this schema shall comply to the requirements as described in the Redfish specification."
                        },
                        "ConnectedSystems@odata.count": {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                        },
                        "ConnectedSystems@odata.navigationLink": {
                            "type": "string",
                            "format": "uri"
                        },
                        "ConnectedSystems": {
                            "type": "array",
                            "items": {
                                "$ref": "http://redfish.dmtf.org/schemas/v1/ComputerSystem.json#/definitions/ComputerSystem"
                            },
                            "readonly": false,
                            "description": "An array of references to the ComputerSystems connected to this interface.",
                            "longDescription": "The value of this property shall be an array of references to the resources that represent the systems connected to this interface and shall be of type ComputerSystem."
                        },
                        "ConnectedDevices@odata.count": {
                            "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                        },
                        "ConnectedDevices@odata.navigationLink": {
                            "type": "string",
                            "format": "uri"
                        },
                        "ConnectedDevices": {
                            "type": "array",
                            "items": {
                                "$ref": "http://redfish.dmtf.org/schemas/v1/PCIeDevice.json#/definitions/PCIeDevice"
                            },
                            "readonly": false,
                            "description": "An array of references to the PCIeDevices connected to this interface.",
                            "longDescription": "The value of this property shall be an array of references to the resources that represent the PCIe devices connected to this interface and shall be of type PCIeDevice."
                        }
                    },
                    "readonly": true,
                    "description": "The links object contains the links to other resources that are related to this resource."
                },
                "Width": {
                    "type": [
                        "number",
                        "null"
                    ],
                    "readonly": true,
                    "description": "operating width of the HSSInterface specified in number of lanes",
                    "longDescription": "The value of this property shall contain the operating width of the HSSInterface specified in number of lanes."
                },
                "ContainingSystem": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/ComputerSystem.json#/definitions/ComputerSystem",
                    "readonly": true,
                    "description": "The ComputerSystem hosting this HSSInterface",
                    "longDescription": "This object shall only contain a reference to a ComputerSystem schema."
                },
                "Connections@odata.count": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/count"
                },
                "Connections@odata.navigationLink": {
                    "type": "string",
                    "format": "uri"
                },
                "Connections": {
                    "type": "array",
                    "items": {
                        "$ref": "http://redfish.dmtf.org/schemas/v1/HSSConnection.json#/definitions/HSSConnection"
                    },
                    "readonly": false,
                    "description": "An array of references to the connections that use this interface.",
                    "longDescription": "The value of this property shall be an array of references to the resources that represent the connections that use this interface and shall be of type HSSConnection."
                }
            },
            "required": [
                "Id",
                "Name"
            ],
            "description": "This is the schema definition for the HSSInterface resource.",
            "longDescription": "This is the schema definition for the HSSInterface resource. It represents the properties of a HSSInterface."
        }
    },
    "copyright": "Copyright 2014-2017 Distributed Management Task Force, Inc. (DMTF). For the full DMTF copyright policy, see http://www.dmtf.org/about/policies/copyright"
}
