{
    "$schema": "http://redfish.dmtf.org/schemas/v1/redfish-schema.v1_2_0.json",
    "title": "#ChristmannExtension.v0_1_0",
    "definitions": {
        "Chassis": {
            "type": "object",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "additionalProperties": false,
            "properties": {
                "Location": {
                    "anyOf": [
                        {"$ref": "#/definitions/Location"},
                        {"type": "null"}
                    ]
                },
                "Settings": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Settings.json#/definitions/Settings",
                    "readonly": true,
                    "description": "A reference to the Settings for this chassis.",
                    "longDescription": "The value of this property shall be a link to Settings."
                }
            },
            "description": "Christmann Chassis extension",
            "longDescription": "Christmann Chassis extension"
        },
        "Location": {
            "type": "object",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "additionalProperties": false,
            "properties": {
                "Id": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "description": "String containing physical location ID of this chassis",
                    "longDescription": "String containing physical location ID of this chassis"
                },
                "ParentId": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "description": "String containing  physical location ID of parent chassis",
                    "longDescription": "String containing  physical location ID of parent chassis"
                }
            }
        },
        "StartDeepDiscovery": {
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "title": {
                    "type": "string",
                    "description": "Friendly action name"
                },
                "target": {
                    "type": "string",
                    "format": "uri",
                    "description": "Link to invoke action"
                }
            }
        }
    },
    "copyright": "Copyright 2014-2017 Distributed Management Task Force, Inc. (DMTF). For the full DMTF copyright policy, see http://www.dmtf.org/about/policies/copyright"
}
