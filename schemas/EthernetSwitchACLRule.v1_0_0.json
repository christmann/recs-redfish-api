            "type": "string",
            "enum": [
                "Egress",
                "Ingress",
                "Bidirectional",
                "Redirect"
            ],
            "enumDescriptions": {
                "Egress": "Mirror egressing traffic on the mirrored port to the mirror destination port.",
                "Ingress": "Mirror ingressing traffic on the mirrored port to the mirror destination port.",
                "Bidirectional": "Mirror ingressing and egressing traffic on the mirrored port to the mirror destination port.",
                "Redirect": "Mirror ingress traffic to the mirror destination port and drop the traffic ingressing the mirror ports."
            }
        },
