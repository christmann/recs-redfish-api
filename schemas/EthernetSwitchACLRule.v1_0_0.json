                    "type": [
                        "number",
                        "null"
                    ],
                    "readonly": true,
                    "description": "IP layer 4 protocol number",
                    "longDescription": "IP layer 4 protocol number as defined here: http://www.iana.org/assignments/protocol-numbers/protocol-numbers.xhtml"
