                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name",
                    "readonly": true
                },
                "RuleId": {
                    "type": [
                        "number",
                        "null"
                    ],
                    "readonly": false,
                    "description": "This is ACL rule ID which determine rule priority.",
                    "longDescription": "This is ACL rule ID which determine rule priority. If not provided during creation, service will assign default next free Id"
                },
                "Action": {
                    "anyOf": [
                        {"$ref": "#/definitions/ActionType"},
                        {"type": "null"}
