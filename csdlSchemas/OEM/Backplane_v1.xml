	</edmx:Reference>
	<edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/ComputerSystemCollection_v1.xml">
		<edmx:Include Namespace="ComputerSystemCollection"/>
	</edmx:Reference>
