				<Property Name="Processors" Type="Collection(ComposedNodeAllocationSystemRequirement.v0_1_0.Processor)">
					<Annotation Term="OData.Description" String="The requirements for one or more processors" />
				</Property>
				<Property Name="EthernetInterfaces" Type="Collection(ComposedNodeAllocationSystemRequirement.v0_1_0.EthernetInterface)">
					<Annotation Term="OData.Description" String="The requirements for one or more ethernet interfaces" />
