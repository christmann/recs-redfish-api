		<Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ComposedNodeAllocationDeviceRequirement.v0_1_0">

			<ComplexType Name="ComposedNodeAllocationDeviceRequirement">
				<Annotation Term="OData.AdditionalProperties" Bool="false"/>
